{
  "name": "Sticky Youtube",
  "description": "Embed youtube player in the page",
  "version": "1.0",
  "permissions": ["activeTab", "tabs", "*://*/*"],
  "icons": {
    "48": "images/youtube-48.png"
  },
  "browser_action": {
    "default_title": "DOM Editor",
    "default_popup": "popup.sticky-youtube.html",
    "default_icon": {
      "16": "images/youtube-16.png",
      "24": "images/youtube-24.png",
      "32": "images/youtube-32.png"
    }
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
